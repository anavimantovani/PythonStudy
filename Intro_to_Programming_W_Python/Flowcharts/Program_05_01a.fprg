    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="keepGoing" type="String" array="False" size=""/>
            <declare name="sales" type="Real" array="False" size=""/>
            <declare name="commission" type="Integer" array="False" size=""/>
